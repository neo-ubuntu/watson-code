constraint connect_required_qty_of_sfps on EVA_Emb_FC_Switch_GBIC//EF 12/3/08 conn  - new constraint
{
	exitIf(RESTORING_FROM_SSL);
	exitIf(IN_RERACKING_EVENT); 

	HPQ_HSV300_Half_Controller_w_Switch_Case_Instance ?case_CI;
	SETVALUE(?case_CI, ancestor(?this, HPQ_HSV300_Half_Controller_w_Switch_Case));

	exitIf(NOT(IS_VALID_INSTANCE(?case_CI)));
	exitIf(NOT(Boolean(?case_CI.eva_required_controller_for_kit_C)));

	exitIf(COUNT(SUBSET(?gb, EVA_Emb_FC_Switch_GBIC(descendants(?case_CI)), 
		NOT ?gb.device_connected_to_host_CI.do_not_connect_B)) >= ?case_CI.eva_conn_to_hbas_included_F);

	exitIf(MEMBER(?this.gb_fc_switch_channel_CI, ?thisSystem.possible_channels_mCI));

	SETVALUE(?this.device_connected_to_host_CI.do_not_connect_B, FALSE);
}
