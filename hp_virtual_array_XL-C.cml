constraint eva_expansion_case_rack_assignment on M6412_Case_Base
{
	TRUE;
}

constraint eva_expansion_case_rack_assignment on M6412_Case_Extra
{
	?this.valid_context_subsystem_B() ? //shelf belongs	to a solution
		TRUE
		:
		(component(?this) == hpq_fc_drive_encl_m6412a_case_extra ?
			TRUE
			:
			//standalone hpq_fc_drive_encl_m6412_case_extra - can be only in non-quoted
			SETVALUE(?this.possible_cabinets_mC, {storage_centric_null_rack, power_on_site_null_rack}));
}

constraint info_for_stanalone_m5314_case on	M6412_Case_Extra
{
	TRUE;
}

//Supposedly controller should can be racked in quoted rack to start new field solution, just w/o 0D1 
//constrain class HPQ_HSV300_Controller_Case do_pre_place_case_in_cabinet with eva_controller_rack_assignment;
//Modfied: czulueta The NAS4400 specfic add on shleves are not field options and need 0D1 structure.

constraint change_partnumber_for_standalone_moongazers on M6412_Case_Extra	
{
	String ?vpn_S;
	component(?this) == hpq_fc_drive_encl_m6412_case_extra 
		?	SETVALUE(?vpn_S, "AG638A")
			:	(component(?this) == hpq_fc_drive_encl_m6412a_case_extra 
					?	SETVALUE(?vpn_S, "AG638B")
						: TRUE);

	?this.my_cabinet_CI.sb_ToBeQuoted 
		AND COUNT(factory_integrate(%allInstances)) > 0

		?
		SETVALUE(?this.visible_part_number_S, ?vpn_S + "_EXTRA")
		:
		SETVALUE(?this.visible_part_number_S, ?vpn_S + "_STANDALONE");

	//Field integrated bundles have extra shelves added as field intergrate
	((?this.valid_context_subsystem_B() AND
		(COUNT(hpq_eva_4400_st_kit_146gb_15k_dummy_case_field(?this.my_context_subsystem_CI.my_instances_mCI)) > 0
		OR
		COUNT(hpq_eva_4400_st_kit_400gb_10k_dummy_case_field(?this.my_context_subsystem_CI.my_instances_mCI)) > 0
		OR
		COUNT(hpq_eva_4400_st_kit_blades_dummy_case_field(?this.my_context_subsystem_CI.my_instances_mCI)) > 0
		OR
		COUNT(hpq_eva_4400_st_kit_12x400gb_blades_dummy_case_field(?this.my_context_subsystem_CI.my_instances_mCI)) > 0
		OR
		COUNT(hpq_eva_4400_st_kit_8x146gb_blades_dummy_case_field(?this.my_context_subsystem_CI.my_instances_mCI)) > 0
		OR
		FIRST(HPQ_EVA_Dummy_Case(?this.my_context_subsystem_CI.my_instances_mCI)).field_int_bundle_B
		OR
		COUNT(hpq_eva_4400_dummy_case_field(?this.my_context_subsystem_CI.my_instances_mCI)) > 0
		OR
		COUNT(hpq_eva_4400_w_switch_dummy_case_field(?this.my_context_subsystem_CI.my_instances_mCI)) > 0
		OR
		COUNT(hpq_eva_4400_modb_dummy_case_field(?this.my_context_subsystem_CI.my_instances_mCI)) > 0
		)) 
		AND SETVALUE(?this.visible_part_number_S, ?vpn_S + "_STANDALONE")) OR TRUE;
	

//czulueta structure change for NAS4400 based models.
	(?this.valid_context_subsystem_B() AND
 	(COUNT(hpq_nas_4400_st_kit_12x400gb_blades_dummy_case_field(?this.my_context_subsystem_CI.my_instances_mCI)) > 0 )
 				AND SETVALUE(?this.bundle_with_rack_B, FALSE)) OR TRUE;	
	(?this.valid_context_subsystem_B() AND
 	(COUNT(hpq_nas_4400_st_kit_12x450gb_blades_dummy_case_field(?this.my_context_subsystem_CI.my_instances_mCI)) > 0 )
 				AND SETVALUE(?this.bundle_with_rack_B, FALSE)) OR TRUE;

	//M6412 cannot be ordered in a rack by itself
	//while M6412A can
	exitIf(component(?this) == hpq_fc_drive_encl_m6412_case_extra);
	
	//If added as a standalone
	exitIf(NOT(?this.valid_context_subsystem_B()) AND SETVALUE(?this.bundle_with_rack_B, TRUE));
	
	//If controller is in a different rack - eva8400
	HPQ_HSV110_Controller_M3320_Case_Instance ?contr;
	SETVALUE(?contr, FIRST(HPQ_HSV110_Controller_M3320_Case(?this.my_context_subsystem_CI.my_child_subsystem_instances())));
	?this.my_cabinet_CI == ?contr.my_cabinet_CI ? 
			SETVALUE(?this.bundle_with_rack_B, FALSE) : SETVALUE(?this.bundle_with_rack_B, TRUE);
}
hpq_fc_drive_encl_m6412_case_extra generate_warnings_event constrainedBy change_partnumber_for_standalone_moongazers;
hpq_fc_drive_encl_m6412a_case_extra generate_warnings_event constrainedBy change_partnumber_for_standalone_moongazers; 

//constraint drive_type_qty_check on HPQ_EVA_4400_Dummy_Case 
constraint drive_type_qty_check on HPQ_EVA_LE_Dummy_Case
{
	exitIf(NOT(?this.valid_context_subsystem_B()));
	exitIf(?this.field_int_bundle_B);
	//PDM requirement doesn't say if field integrate models can add any qty of each drive class
	//exitIf(component(?this) == hpq_eva_4400_dummy_case_field);

	Multivalued FATA_M6412_FC_Disk_Drive_Instance ?fata_drv;							 
	Multivalued FATA_M6412_FC_Disk_Drive_Instance ?fata_drv_left;							 
	Multivalued Non_FATA_M6412_FC_Disk_Drive_Instance ?drives;							 
	Multivalued Non_FATA_M6412_FC_Disk_Drive_Instance ?drives_left;							 
	Multivalued SSD_M6412_Disk_Drive_Instance ?ssd_drv;
	Multivalued SSD_M6412_Disk_Drive_Instance ?ssd_drv_left;

	Package_Instance ?package_CI;
	SETVALUE(?package_CI, FIRST(SUBSET(?pack, Package(%allInstances), ?pack.package_case_CI == ?this)));

	//drives being added
	SETVALUE(?fata_drv, FATA_M6412_FC_Disk_Drive(?package_CI.sb_PackageInstances));
	SETVALUE(?drives, Non_FATA_M6412_FC_Disk_Drive(?package_CI.sb_PackageInstances));
	SETVALUE(?ssd_drv, SSD_M6412_Disk_Drive(?package_CI.sb_PackageInstances));

	//Drives Left in configuration
	SETVALUE(?fata_drv_left, SUBSET(?d1, FATA_M6412_FC_Disk_Drive(?this.
		my_context_subsystem_CI.my_child_subsystem_instances()), ?d1.marked_for_deletion_F == 1));
	SETVALUE(?drives_left, SUBSET(?d2, Non_FATA_M6412_FC_Disk_Drive(?this.
		my_context_subsystem_CI.my_child_subsystem_instances()), ?d2.marked_for_deletion_F == 1));
	SETVALUE(?ssd_drv_left, SUBSET(?d3, SSD_M6412_Disk_Drive(?this.
		my_context_subsystem_CI.my_child_subsystem_instances()), ?d3.marked_for_deletion_F == 1));

	(COUNT(?fata_drv) + COUNT(?fata_drv_left) == 0) OR (COUNT(?fata_drv) + COUNT(?fata_drv_left) >= 8);
	(COUNT(?drives) + COUNT(?drives_left) == 0) OR (COUNT(?drives) + COUNT(?drives_left) >= 8);
	(COUNT(?ssd_drv + ?ssd_drv_left) == 0) OR (COUNT(?ssd_drv + ?ssd_drv_left) >= 6);
	NOT((COUNT(?fata_drv + ?fata_drv_left) == 0) AND (COUNT(?drives + ?drives_left) == 0)
		AND (COUNT(?ssd_drv + ?ssd_drv_left) == 0));
}

//A conflict for eCO
//constraint check_valid_drive_combinations on HPQ_EVA_4400_Dummy_Case 
constraint check_valid_drive_combinations on HPQ_EVA_LE_Dummy_Case 
{
	exitIf(NOT(?this.valid_context_subsystem_B()));
	exitIf(?this.field_int_bundle_B);

	Multivalued FATA_M6412_FC_Disk_Drive_Instance ?fata_drv;							 
	Multivalued Non_FATA_M6412_FC_Disk_Drive_Instance ?drives;							 
	Multivalued SSD_M6412_Disk_Drive_Instance ?ssd_drv;

	SETVALUE(?fata_drv, FATA_M6412_FC_Disk_Drive(?this
		.my_context_subsystem_CI.my_child_subsystem_instances()));
	SETVALUE(?drives, Non_FATA_M6412_FC_Disk_Drive(?this
		.my_context_subsystem_CI.my_child_subsystem_instances()));
	SETVALUE(?ssd_drv, SSD_M6412_Disk_Drive(?this
		.my_context_subsystem_CI.my_child_subsystem_instances()));

	exitIf((COUNT(?fata_drv) == 0 OR COUNT(?fata_drv) >= 8)
		AND (COUNT(?drives) == 0 OR COUNT(?drives) >= 8)
			AND (COUNT(?ssd_drv) == 0 OR COUNT(?ssd_drv) >= 6));

	MESSAGE("[Error] At least 8 drives of FC/FATA technology or 6 drives of SSD technology should be configured.");
}

