constraint bring_in_default on EVA_HDD_Bulkpack_Case
{
	exitIf(?this.previously_installed_B);

	Component ?hd_C;
	Float ?count_F;

	Boolean(?this.my_clone_target_CI)?
		(
			SETVALUE(?hd_C, component(FIRST(HPQ_FC_Disk_Drive(descendants(?this.my_clone_target_CI)))))
			AND
			SETVALUE(?count_F, COUNT(HPQ_FC_Disk_Drive(descendants(?this.my_clone_target_CI))))
		)
		:
		(
			SETVALUE(?hd_C, FIRST(?this.my_model_C.ok_internal_disk_mC))
			AND
			SETVALUE(?count_F, 10)
		);

	exitIf(NOT Boolean(?hd_C));

	Multivalued Component ?comps_to_add_mC;
	SETVALUE(?comps_to_add_mC, expand_components_mC(?hd_C, ?count_F));
	
	requiresComponent(Generic_Add_Qty_Of_Components_Package_Instance({}),
					  {generic_add_qty_of_components_package},
					  Resource(), ?comp, NoDependency,
					  Initializations(my_subsystem_CI = ?this.my_subsystem_CI,
					  package_instance_CI = ?this,
					  adds_mC = ?comps_to_add_mC));

	SETVALUE(?this.previously_installed_B,TRUE);
}
constrain class EVA_HDD_Bulkpack_Case with bring_in_default;


