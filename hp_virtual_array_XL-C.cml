constraint check_max_for_AG784A on SVSP_Dummy_Case
{	
	Float ?ag784a_count_F;
	Float ?hw_count_F;

 	SETVALUE(?ag784a_count_F, COUNT(hp_svsp_dpm_4port_upgrade_ltu(%allInstances)));

 	SETVALUE(?hw_count_F, COUNT(SVSP_Hardware_Requester(%allInstances))
						+ COUNT(Data_Path_Module(%allInstances)));
	exitIf(?ag784a_count_F <=  3 * ?hw_count_F);

	MESSAGE("[WARNING] AG784A - max 3 per AG781B or AP750A"
			);
}
constrain class SVSP_Dummy_Case generate_warnings_event with check_max_for_AG784A;


//for SVSP Installation and Startup Requirment
constraint ap750a_storage_virtualization_needs_service on Data_Path_Module
{

//exit if Fixed Services are ordered
exitIf(
COUNT(Fixed_or_Flexible_Support_Type(%allInstances)) > 0
AND
NOT(component(FIRST(Fixed_or_Flexible_Support_Type(%allInstances))) == flexible_support_type)
);

//get all subsystems
Multivalued CIM_SubSystem_Instance ?subsys_mCI;
SETVALUE(?subsys_mCI, ?this.get_purple_cats_subsystems_mCI());

exitIf(COUNT(?subsys_mCI) == 0);


FORALL(?subs, ?subsys_mCI,
COUNT(installation_and_startup_package_1y(?subs.my_instances_mCI)) //HA114A1

== 0) 

? MESSAGE("[Error]When ordering AP750A, it is highly recommended" 
+ " that HA114A1 #55L Installation and Startup Service is ordered." )
: TRUE; 

}

constrain class Data_Path_Module generate_warnings_event with ap750a_storage_virtualization_needs_service;


