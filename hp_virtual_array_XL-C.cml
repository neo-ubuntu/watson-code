	exitIf(COUNT(c13_power_input(descendants(?this))) == 0);
	SETVALUE(?thisSystem.pdu_alternatives_mC, SORT(?pdu, ?thisSystem.pdu_alternatives_mC, 
		//MEMBER(?pdu, {high_voltage_10k_16amps_ww_racked_pdu}) ? 1000 : 0));
		classOf(?pdu) <= HPQ_10K_Modular_Racked_PDU 
			AND ?pdu.Amperage_Resource.resourceQuantity == 16 ? 1000 : 0));
}
