constraint eva4400_emb_switch_must_connect_to_kits_controller on HPQ_HSV300_Half_Controller_w_Switch_Case
{
	exitIf(RESTORING_FROM_SSL); 
	exitIf(IN_RERACKING_EVENT);
	exitIf(NOT(Boolean(?this.eva_required_controller_for_kit_C)));
	
//  	Multivalued HSV300_SFP_Instance ?gb_mCI;
//  	SETVALUE(?gb_mCI, SUBSET(?x, HSV300_SFP(?this.my_subsystem_CI.my_instances_mCI), 
//  	                      Boolean(?x.device_connected_to_host_CI.current_cs_CI) AND
//  	                      component(ancestor(?x.device_connected_to_host_CI.current_cs_CI.cs_endpoint_CI,CIM_Storage_Controller))
//  	                                == ?this.eva_required_controller_for_kit_C));
	Multivalued Array_Processor_Instance ?gb_mCI;
	SETVALUE(?gb_mCI, SUBSET(?x, Array_Processor(?this.my_subsystem_CI.my_instances_mCI), 
	                      Boolean(?x.device_connected_to_host_CI.current_cs_CI) AND
	                      component(ancestor(?x.device_connected_to_host_CI.current_cs_CI.cs_endpoint_CI,CIM_Storage_Controller))
	                                == ?this.eva_required_controller_for_kit_C));


	exitIf(COUNT(?gb_mCI) >= ?this.eva_conn_to_hbas_included_F);

	SETVALUE(?thisSystem.possible_channels_mCI, SUBSET(?ch, ?thisSystem.possible_channels_mCI, 
				IS_VALID_INSTANCE(ancestor(?ch, CIM_Storage_Controller)) 
					AND component(ancestor(?ch, CIM_Storage_Controller)) == ?this.eva_required_controller_for_kit_C
						//One connection to a multichannel HBA - EF 12/3/08 conn 
						AND FORALL(?d, descendants(ancestor(?ch, CIM_Storage_Controller)), COUNT(?d.connected_to_me_mCI) == 0)
					)); 
}
