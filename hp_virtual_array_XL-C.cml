constraint warn_cv_eva_media_not_needed_with_eltu on HPQ_Smart_Start_Media
{
	Multivalued Simple_LTU_Software_Instance ?eltu_mCI;

	// If no E-LTUs are present, no message is needed.
	SETVALUE(?eltu_mCI, SUBSET(?e, HPQ_BC_Software(%allInstances)
									+ HPQ_CV_EVA_Software(%allInstances)
