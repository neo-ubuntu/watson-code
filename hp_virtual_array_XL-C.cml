// NAS 4400 comes bundled with PDU's, other PDU's should not be selected
constraint check_nas4400_pdus on NAS_4400_ForBlade_Bundle_Dummy_Case
{
	Multivalued HP_Case_Power_Input_Instance ?my_inputs_mCI;
	Multivalued HP_Power_Connect_Spec_Instance ?my_cs_mCI;
	Multivalued HPQ_PDU_Instance ?my_pdus_mCI;

	exitIf( COUNT(?this.preferred_pdu_mC) < 1 );

	// Find power inputs for EVA4400 rackable components
	FORALL(?eva4400, SUBSET(?e, HP_EVA_Storage_Case(%allInstances),
							?e.my_context_subsystem_CI == ?this.my_context_subsystem_CI),
					SETVALUE(?my_inputs_mCI, ?my_inputs_mCI +
							 HP_Case_Power_Input(descendants(?eva4400))));

	// Find connect specs for EVA4400 rackable components
	FORALL(?input, ?my_inputs_mCI,
					NOT(Boolean(HP_Power_Connect_Spec(?input.current_cs_CI)))
					OR
					SETVALUE(?my_cs_mCI, ?my_cs_mCI +
							 {HP_Power_Connect_Spec(?input.current_cs_CI)}));

	// Find pdus for EVA4400 rackable components
	FORALL(?cs, ?my_cs_mCI,
					NOT(Boolean(HPQ_PDU(?cs.cs_endpoint_CI)))
					OR
					SETVALUE(?my_pdus_mCI, ?my_pdus_mCI +
							 {HPQ_PDU(?cs.cs_endpoint_CI)}));

	exitIf(FORALL(?pdu, ?my_pdus_mCI,
					MEMBER(component(?pdu), ?this.preferred_pdu_mC)));

    MESSAGE("[ERROR] NAS4400 solution comes with HP 24A High Voltage Modular PDUs (252663-D72 or 252663-B31). Additional PDU's should not be used instead.");
}
constrain class NAS_4400_ForBlade_Bundle_Dummy_Case generate_warnings_event with check_nas4400_pdus;


