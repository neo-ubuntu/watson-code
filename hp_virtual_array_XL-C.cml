constraint set_subsystem_for_SVSP_software on SVSP_Software
failureMessage: "Failure to set subsystem on " + ?this.description;
{	
 	exitIf(NOT(Boolean(?this.my_subsystem_CI)));

	CIM_SubSystem_Instance ?subsys;
	SETVALUE(?subsys, ?this.my_subsystem_CI);
	
	exitIf(COUNT(?subsys.connect_to_subsystems_mCI) == 0);
	
	// get subsystem we'll be using
	CIM_SubSystem_Instance ?new_subsys;
	SETVALUE(?new_subsys,
			 FIRST(SORT(?ss, CIM_SubSystem(?subsys.connect_to_subsystems_mCI), 
			 	COUNT(Storage_Centric_Model(?ss.my_instances_mCI)) > 0 ? 100 : 0))); 

 	SETVALUE(?this.my_subsystem_CI, ?new_subsys);
	
	FORALL(?desc, descendants(?this), SETVALUE(?desc.my_subsystem_CI, ?new_subsys));
}
constrain class SVSP_Software do_post_finish_configuration_event with set_subsystem_for_SVSP_software;


