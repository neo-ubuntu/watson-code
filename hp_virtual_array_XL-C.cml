//SETVALUE(?case_CI, FIRST(HSV300_Half_Contr_w_Switch_SAN_Case(?this.my_subsystem_CI.my_instances_mCI)));
	HPQ_HSV300_Half_Controller_w_Switch_Case_Instance ?case_CI;
	SETVALUE(?case_CI, FIRST(HPQ_HSV300_Half_Controller_w_Switch_Case(?this.my_subsystem_CI.my_instances_mCI)));

	exitIf(NOT(Boolean(?case_CI)));
	exitIf(NOT(Boolean(?case_CI.eva_required_controller_for_kit_C)));

//  	Multivalued HSV300_SFP_Instance ?gb_mCI;
//  	SETVALUE(?gb_mCI, SUBSET(?x, HSV300_SFP(?this.my_subsystem_CI.my_instances_mCI), 
