///////////////////////////////////////////////////////////////////////////////////////////////////////////////////
// Racked EVAs without PDUs (where the user has deleted the default PDUs) require FE2 a-la-carte service HA864A1.
// Bring it in if necessary.
///////////////////////////////////////////////////////////////////////////////////////////////////////////////////
constraint eva_rack_without_pdus_requires_fe2_svc on HP_EVA_Storage_Case
{
	// Don't bring in the service for field integrated bundles.
	HPQ_EVA_Dummy_Case_Instance ?dummy_case_CI;
	NOT(Boolean(?this.my_context_subsystem_CI))
	OR
	SETVALUE(?dummy_case_CI, FIRST(HPQ_EVA_Dummy_Case(?this.my_context_subsystem_CI.my_instances_mCI)));
	exitIf(Boolean(?dummy_case_CI) AND ?dummy_case_CI.field_int_bundle_B);  

    // Does the EVA need this service?
    exitIf(NOT(MEMBER(fe2_custom_placement_within_rack, ?this.suggested_associated_components_mC)));

	// Has the user explicitly removed the service from this EVA instance before, or from other
	// EVA instances in the configuration before?  This check prevents HA864A1 from being brought in every time 
	// at finish_configuration_event even after the user has removed it.  We wouldn't have to check this if
	// this constraint only ran when the EVA case was created, but at that point the PDUs might not be there yet
	// so we have to wait and run at finish_configuration_event and do this extra checking.
    exitIf(MEMBER(fe2_custom_placement_within_rack, ?this.components_removed_from_this_instance_mC));
    exitIf(THEREEXISTS(?c, HP_EVA_Storage_Case(%allInstances), 
                       ?c != ?this 
                       AND 
                       MEMBER(fe2_custom_placement_within_rack, ?c.components_removed_from_this_instance_mC)));

    HP_Rack_Instance ?rack_CI;
    SETVALUE(?rack_CI, ancestor(?this, HP_Rack));

	// We only care about racked EVAs with 0 PDUs in the rack.
    exitIf(NOT(Boolean(?rack_CI)));
    exitIf(classOf(?rack_CI) <= Non_Quoted_Rack);
    exitIf(NOT(?rack_CI.sb_ToBeQuoted));
    exitIf(COUNT(SUBSET(?pdu, HPQ_Modular_PDU(descendants(?rack_CI)), ?pdu.sb_ToBeQuoted)) != 0);

	// Check whether we already have HA864A1.
    exitIf(COUNT(fe2_custom_placement_within_rack(descendants(?rack_CI))) > 0);

	// Bring in HA864A1.
    requiresComponent(Component_Instance({}), {fe2_custom_placement_within_rack}, Resource(), ?svc, NoDependency);

	MESSAGE("[INFO] " + ?svc.description + " has been added to this order.");
}

component eva_rack_without_pdus_requires_fe2_svc_msg: Component 
{
productLines: HP;
}
eva_rack_without_pdus_requires_fe2_svc_msg constrainedBy always_false;

///////////////////////////////////////////////////////////////////////////////////////////////////////////////////
// Racked EVAs without PDUs (where the user has deleted the default PDUs) require FE2 a-la-carte service HA864A1.
// Warn the user about this.
///////////////////////////////////////////////////////////////////////////////////////////////////////////////////
constraint eva_rack_without_pdus_requires_fe2_svc_warning on HP_EVA_Storage_Case
{
	// Don't show the message for field integrated bundles.
	HPQ_EVA_Dummy_Case_Instance ?dummy_case_CI;
	NOT(Boolean(?this.my_context_subsystem_CI))
	OR
	SETVALUE(?dummy_case_CI, FIRST(HPQ_EVA_Dummy_Case(?this.my_context_subsystem_CI.my_instances_mCI)));
	exitIf(Boolean(?dummy_case_CI) AND ?dummy_case_CI.field_int_bundle_B);  

    // Does the EVA need this service?
    exitIf(NOT(MEMBER(fe2_custom_placement_within_rack, ?this.suggested_associated_components_mC)));

    String ?message_S;                                                                                     
    HP_Rack_Instance ?rack_CI;
    SETVALUE(?rack_CI, ancestor(?this, HP_Rack));

	// We only care about racked EVAs with 0 PDUs in the rack.
    exitIf(NOT(Boolean(?rack_CI)));
    exitIf(classOf(?rack_CI) <= Non_Quoted_Rack);
    exitIf(NOT(?rack_CI.sb_ToBeQuoted));
    exitIf(COUNT(SUBSET(?pdu, HPQ_Modular_PDU(descendants(?rack_CI)), ?pdu.sb_ToBeQuoted)) != 0);

	// Show the message whether or not HA864A1 is in the configuration.
    SETVALUE(?message_S, "[WARNING] Orders that do not include the recommended number of approved PDUs must include Factory Express part # HA864A1 to account for changes in rack configuration related to the removal of the PDUs.");
    exitIf(REMAINING(eva_rack_without_pdus_requires_fe2_svc_msg) > 0);                                                          
    requestComponent(eva_rack_without_pdus_requires_fe2_svc_msg, 1, 0, TRUE, Initializations(failure_message_S = ?message_S));  
}

constrain class HP_EVA_Storage_Case finish_configuration_event with eva_rack_without_pdus_requires_fe2_svc;
constrain class HP_EVA_Storage_Case generate_warnings_event    with eva_rack_without_pdus_requires_fe2_svc_warning;

////////////////////////////////////////////////////////////////////////////////////////////////////////////
/// If user adds HA864A1 explicitly to an EVA rack, and HA864A1 might have been brought in automatically
// earlier because the rack had no PDUs, make sure that the EVA case is marked to show that the user did not 
// explicitly remove it.
// This is really resetting a "flag" (in this case a set that may or may not be empty) to on (empty).
// Constraint is modeled on constraint mark_suggested_component_added in hp_sw_unix.cml.
////////////////////////////////////////////////////////////////////////////////////////////////////////////
constraint mark_fe2_svc_explicitly_added_to_eva_rack on Component
{
    FORALL(?c, HP_EVA_Storage_Case(%allInstances), 
           NOT(MEMBER(component(?this), ?c.suggested_associated_components_mC))  // This is irrelevant if there is no EVA in the config that has this service recommended.
           OR SETVALUE(?c.components_removed_from_this_instance_mC, ?c.components_removed_from_this_instance_mC - {component(?this)}));
}
fe2_custom_placement_within_rack constrainedBy mark_fe2_svc_explicitly_added_to_eva_rack;

////////////////////////////////////////////////////////////////////////////////////////////////////////////////
// If user removes HA864A1 from an EVA rack, and HA864A1 might have been brought in automatically
// because it is in the list of suggested associated components for a software component, make sure that
// earlier because the rack had no PDUs, make sure that the EVA case is marked to show that the user explicitly 
// removed the service and it shouldn't automatically be brought in for new EVAs.
// This is really resetting a "flag" (in this case a set that may or may not be empty) to off (not empty).
// Constraint is modeled on constraint mark_suggested_component_removed in hp_sw_unix.cml.
////////////////////////////////////////////////////////////////////////////////////////////////////////////////
constraint mark_fe2_svc_explicitly_removed_from_eva_rack on Component
{
    FORALL(?c, HP_EVA_Storage_Case(%allInstances), 
           NOT(MEMBER(component(?this), ?c.suggested_associated_components_mC))  // This is irrelevant if there is no EVA in the config that has this service recommended.
           OR SETVALUE(?c.components_removed_from_this_instance_mC, ?c.components_removed_from_this_instance_mC + {component(?this)}));
}
fe2_custom_placement_within_rack do_worksheet_predelete constrainedBy mark_fe2_svc_explicitly_removed_from_eva_rack;

