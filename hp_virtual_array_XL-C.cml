function Boolean custom_integration_state on M6412_Case_Extra()
{

((Boolean(?this.my_cabinet_CI) AND ?this.my_cabinet_CI.sb_ToBeQuoted)
	AND	?this.valid_context_subsystem_B() AND 
	(COUNT(hpq_eva_4400_dummy_case_field(?this.my_context_subsystem_CI.my_instances_mCI)) == 0
		AND COUNT(hpq_eva_4400_w_switch_dummy_case_field(?this.my_context_subsystem_CI.my_instances_mCI)) == 0
		AND COUNT(hpq_eva_4400_st_kit_146gb_15k_dummy_case_field(?this.my_context_subsystem_CI.my_instances_mCI)) == 0
		AND COUNT(hpq_eva_4400_st_kit_400gb_10k_dummy_case_field(?this.my_context_subsystem_CI.my_instances_mCI)) == 0
		AND COUNT(hpq_eva_4400_st_kit_blades_dummy_case_field(?this.my_context_subsystem_CI.my_instances_mCI)) == 0
		AND COUNT(hpq_eva_4400_st_kit_blades_dummy_case_field(?this.my_context_subsystem_CI.my_instances_mCI)) == 0
		AND COUNT(hpq_eva_4400_st_kit_12x400gb_blades_dummy_case_field(?this.my_context_subsystem_CI.my_instances_mCI)) == 0
		AND COUNT(hpq_eva_4400_st_kit_8x146gb_blades_dummy_case_field(?this.my_context_subsystem_CI.my_instances_mCI)) == 0
		AND COUNT(hpq_eva_4400_modb_dummy_case_field(?this.my_context_subsystem_CI.my_instances_mCI)) == 0
		AND NOT(FIRST(HPQ_EVA_Dummy_Case(?this.my_context_subsystem_CI.my_instances_mCI)).field_int_bundle_B)
	))
	OR 
	(NOT(?this.valid_context_subsystem_B()) AND Boolean(?this.my_cabinet_CI) AND ?this.my_cabinet_CI.sb_ToBeQuoted) 
	OR 
	SETVALUE(?this.integration_state_S, "FIELD");

return(TRUE);
}

function Boolean custom_integration_state on M6412_Case_Base()
{

NOT(COUNT(hpq_eva_4400_st_kit_146gb_15k_dummy_case_field(?this.my_context_subsystem_CI.my_instances_mCI)) > 0
	OR
	FIRST(HPQ_EVA_Dummy_Case(?this.my_context_subsystem_CI.my_instances_mCI)).field_int_bundle_B 
	OR
	COUNT(hpq_eva_4400_st_kit_400gb_10k_dummy_case_field(?this.my_context_subsystem_CI.my_instances_mCI)) > 0
	OR 
	COUNT(hpq_eva_4400_st_kit_blades_dummy_case_field(?this.my_context_subsystem_CI.my_instances_mCI)) > 0
	OR
	COUNT(hpq_eva_4400_st_kit_12x400gb_blades_dummy_case_field(?this.my_context_subsystem_CI.my_instances_mCI)) > 0
	OR
	COUNT(hpq_eva_4400_st_kit_8x146gb_blades_dummy_case_field(?this.my_context_subsystem_CI.my_instances_mCI)) > 0
	) 
	OR SETVALUE(?this.integration_state_S, "FIELD");

return(TRUE);
}

