component hp_eva8400_exp_rack_cable_kit: EVA8400_Exp_Rack_Kit
{
productLines: HP; 
label: "HP EVA8400 Expansion Rack Accessory Kit";
description: "HP EVA8400 Expansion Rack Accessory Kit" +" ("+ string(?this.visible_part_number_S)+")";
values:
	visible_part_number_S = "AP712A";
	container_resource_required_R = Option_Container_Resource;
}
hp_eva8400_exp_rack_cable_kit constrainedBy generic_requires_container_without_consumption;

//is introduced on the Kit, so it should be placed under rack
constraint place_in_racks_option_container on EVA8400_Exp_Rack_Kit
{
	exitIf(IS_VALID_INSTANCE(?this.sb_ContainerDestiny));
	
	Multivalued HP_10KG2_Series_Rack_Instance ?racks;
	SETVALUE(?racks, SORT(?x, HP_10KG2_Series_Rack(%allInstances), 
		COUNT(SUBSET(?case, M6412_Case_Extra(?x.my_racked_cases_mCI), ?case.bundle_with_rack_B)))); 
	
	exitIf(COUNT(?racks) == 0);
	
	HP_10KG2_Series_Rack_Instance ?rack;
	SETVALUE(?rack, FIRST(?racks));
	
	SETVALUE(?this.sb_ContainerDestiny, ?rack.option_container_CI);	
}
hp_eva8400_exp_rack_cable_kit do_compute_container_destiny constrainedBy place_in_racks_option_container;


constraint get_eva8400_exp_rack_cable_kit on HP_10KG2_Series_Rack
{
	SETVALUE(?this.subsystems_inside_rack_mCI, ?this.subsystems_inside_rack_mCI); 
   	exitIf(COUNT(SUBSET(?k, hp_eva8400_exp_rack_cable_kit(%allInstances), ?k.sb_ContainerDestiny == ?this.option_container_CI)) > 0);	
	requiresComponent(EVA8400_Exp_Rack_Kit_Instance({}), {hp_eva8400_exp_rack_cable_kit}, Resource(), ?kit, 
		NoDependency, Initializations(sb_ContainerDestiny = ?this.option_container_CI, sb_ToBeQuoted = FALSE)); 
}

constraint quote_eva8400_exp_rack_cable_kit on HP_10KG2_Series_Rack
{
	EVA8400_Exp_Rack_Kit_Instance ?exp_kit;

	Multivalued M6412_Case_Extra_Instance ?shelves;
	Multivalued M6412_Case_Extra_Instance ?ind_shelves;

	
	SETVALUE(?exp_kit, FIRST(hp_eva8400_exp_rack_cable_kit(?this.option_container_CI.sb_Contents)));

	exitIf(NOT(IS_VALID_INSTANCE(?exp_kit)));
	
	SETVALUE(?exp_kit.sb_ToBeQuoted, FALSE);
							
	SETVALUE(?shelves, SUBSET(?c, M6412_Case_Extra(?this.my_racked_cases_mCI), 
				?c.sb_ToBeQuoted AND ?c.bundle_with_rack_B));//all M6412 with no context or solution in another rack
	
	exitIf(COUNT(?shelves) == 0);

	SETVALUE(?ind_shelves, SUBSET(?cs, ?shelves, NOT(?cs.valid_context_subsystem_B())));

	//Non-context shelves are assumed to be expansion
	//for existing 2c18d and hence they need kits too.
	Float ?qty;
	Float ?B_count;
	
	SETVALUE(?qty, COUNT(M6412_Case_Extra(?ind_shelves))); 
	
	?qty > 0 
		? (?qty <= 9 //less then maximum expansion to 2c27d is 9 shelves 
			? (?qty <= 3 
				? SETVALUE(?B_count, ?qty) : SETVALUE(?B_count, 3)
			  )
			: (?qty - 9 <= 3 
				? SETVALUE(?B_count, 3 + ?qty - 9) : SETVALUE(?B_count, 3 + 3)
			  )
		   )
		: SETVALUE(?B_count, 0);

	//Need 3 kits for first 3 shelves for each context_subsystem in the rack.   
	Float ?qty_cont;
	Float ?C_count;
	
	FORALL(?sub, ?this.subsystems_inside_rack_mCI, 
		SETVALUE(?qty_cont, COUNT(SUBSET(?sh, ?shelves, 
							?sh.valid_context_subsystem_B() AND ?sh.my_context_subsystem_CI == ?sub)))
		AND
		(?qty_cont > 0 
				? (?qty_cont <= 3 ? SETVALUE(?C_count, ?C_count + ?qty_cont) : SETVALUE(?C_count, ?C_count + 3))
					: SETVALUE(?C_count, ?C_count + 0)
		)
	);

	(?this.sb_ToBeQuoted
		AND SETVALUE(?exp_kit.sb_ToBeQuoted, TRUE)
			AND SETVALUE(?exp_kit.sb_Quantity, ?B_count + ?C_count))
					OR TRUE;
}

