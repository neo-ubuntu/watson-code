constraint set_dont_quote_on_included_hbas on HPQ_EVA_4400_Bundle_Dummy_Case
{

Multivalued Component_Instance ?con_inst;
SETVALUE(?con_inst, ?this.my_context_subsystem_CI.my_context_instances_child_mCI()); 

Multivalued CIM_Storage_Controller_Instance ?hbas;

// HSV300_Half_Contr_w_Switch_SAN_Case_Instance ?case;
// SETVALUE(?case, FIRST(HSV300_Half_Contr_w_Switch_SAN_Case(?con_inst)));                            
HPQ_HSV300_Half_Controller_w_Switch_Case_Instance ?case;
SETVALUE(?case, FIRST(HPQ_HSV300_Half_Controller_w_Switch_Case(?con_inst)));                            

exitIf(NOT(Boolean(?case)));

// Multivalued HSV300_SFP_Device_Instance ?devices;
// SETVALUE(?devices, HSV300_SFP_Device(?con_inst));
Multivalued HP_Fibre_Device_Instance ?devices;
SETVALUE(?devices, HP_Fibre_Device(?con_inst));

SETVALUE(?devices, SUBSET(?d, ?devices, Boolean(?d.current_cs_CI) AND Boolean(?d.current_cs_CI.cs_endpoint_CI)
	AND component(ancestor(?d.current_cs_CI.cs_endpoint_CI, CIM_Storage_Controller)) 
		== ?case.eva_required_controller_for_kit_C));

Float ?i;
FORALL(?dev, ?devices, (SETVALUE(?i, ?i + 1) AND ?i > ?case.eva_conn_to_hbas_included_F) 
	OR 
	(SETVALUE(ancestor(?dev.current_cs_CI.cs_endpoint_CI, CIM_Storage_Controller).sb_ToBeQuoted, FALSE)//HBAs included
	AND SETVALUE(?dev.current_cs_CI.cs_connector_CI.sb_ToBeQuoted, FALSE)));//Cables are included
}
constrain class HPQ_EVA_4400_Bundle_Dummy_Case pre_bundling_event with set_dont_quote_on_included_hbas;

