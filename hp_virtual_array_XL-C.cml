constraint update_choice_data on EVA4400_SFP_Choice
{
	Component_Instance ?sel_ov_inst_CI;
   	SETVALUE(?sel_ov_inst_CI, ?this.get_selected_instance_CI());

	Multivalued Component_Instance ?inst;
	NOT(?sel_ov_inst_CI.valid_context_subsystem_B()) OR SETVALUE(?inst, ?sel_ov_inst_CI.my_context_subsystem_CI.my_child_subsystem_instances());
	
	COUNT(hpq_hsv300_half_controller_w_8gb_switch_case(?inst) + hpq_hsv300_half_controller_w_switch_case(?inst)
		+ hsv300_contr_w_switch_san_case(?inst)
		+ hsv300_modc_contr_w_switch_san_case(?inst)) > 0 
		? SETVALUE(?this.control_help_S, "Each added SFP will make a connection to a host.  A cable will be added for each connection.")
			AND SETVALUE(?this.min_value_F, 0)
// 			: (COUNT(hpq_hsv300_half_controller_w_switch_case_new(?inst)) > 0
// 					? SETVALUE(?this.my_components_mC, {eva_emb_switch_4Gb_sfp})
// 						: SETVALUE(?this.my_components_mC, {eva_emb_switch_8Gb_sfp}));	
		:
		SETVALUE(?this.my_components_mC, {eva_emb_switch_8Gb_sfp, eva_emb_switch_4Gb_sfp, eva_emb_switch_8Gb_LW_sfp});	
}
