	exitIf(NOT(IS_VALID_INSTANCE(?rack)));
	exitIf(NOT(?this.sb_ToBeQuoted));

	Multivalued Proliant_Cclass_Blade_Server_Enclosure_Instance ?chassis;
	//SETVALUE(?chassis, SUBSET(?ch, Proliant_Cclass_Blade_Server_Chassis(descendants(?rack)), ?ch.sb_ToBeQuoted));
	SETVALUE(?chassis, SUBSET(?ch, Proliant_Cclass_Blade_Server_Enclosure(descendants(?rack)), ?ch.sb_ToBeQuoted));
	exitIf(COUNT(?chassis) == 0);//the only way to configure w/o blades is Storage Centric, no error for SC models
	
	exitIf((?this.cto_bundle_B AND COUNT(SUBSET(?c, ?chassis, ?c.cto_proliant_chassis_B)) > 0) 
		OR (NOT(?this.cto_bundle_B) AND COUNT(SUBSET(?c1, ?chassis, NOT(?c1.cto_proliant_chassis_B))) > 0));

	String ?message;
	?this.cto_bundle_B ? SETVALUE(?message, ?this.description + " requires a quoted CTO Blade Enclosure in the SAME Rack.") 
	: SETVALUE(?message, ?this.description + " requires a quoted BTO Blade Enclosure in the SAME Rack.");

	MESSAGE("[ERROR] " + ?message);
}
